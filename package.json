{
  "name": "intact",
  "description": "A very lightweight data-binding library to create rich web application",
  "version": "2.2.0",
  "main": "dist/index",
  "browser": "dist/intact.js",
  "module": "src/index",
  "scripts": {
    "test": "karma start karma.conf.saucelabs.js",
    "test:local": "karma start karma.conf.js",
    "release": "npm run release-patch",
    "prelease": "npm version prerelease && git push --tags --force && git push && npm publish",
    "release-patch": "git checkout master && npm version patch && git push --tags && git push && npm publish",
    "release-minor": "git checkout master && npm version minor && git push --tags && git push && npm publish",
    "release-major": "git checkout master && npm version major && git push --tags && git push && npm publish",
    "build": "rollup -c rollup.config.js && rollup -c rollup.config.node.js && uglifyjs dist/intact.js -o dist/intact.min.js -m"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Javey/Intact"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "core-js": "^2.5.0",
    "css-loader": "^0.28.4",
    "es6-promise": "^4.1.1",
    "express": "^4.13.1",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.7.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-webpack": "^2.0.3",
    "lodash": "^4.17.4",
    "mocha": "^2.4.5",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-watch": "^3.2.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.18.2",
    "vdt-loader": "^1.0.0",
    "webpack": "^2.4.1"
  },
  "dependencies": {
    "misstime": "^0.3.1",
    "vdt": "^1.2.3"
  }
}
