{
  "name": "intact-builder",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "benchmark": "karma start fixtures/karma.benchmark.js",
    "test": "karma start fixtures/karma.test.js",
    "snapshot": "UPDATE=1 npm run test",
    "clean": "rimraf node_modules/.rpt2_cache && lerna exec -- rimraf dist",
    "prebuild": "npm run clean",
    "build": "run-p build:** && npm run types",
    "build:es:dev": "            npm run rollup -- --env=development --name=index --ext=esm.dev.js          --format=es   --minify=false",
    "build:es:prod": "           npm run rollup -- --env=production  --name=index --ext=esm.prod.js         --format=es   --minify=false",
    "build:es": "                npm run rollup -- --env=unknown     --name=index --ext=esm.js              --format=es   --minify=false --replace=false",
    "build:cjs:dev": "           npm run rollup -- --env=development --name=index --ext=cjs.dev.js          --format=cjs  --minify=false",
    "build:cjs:prod": "          npm run rollup -- --env=production  --name=index --ext=cjs.prod.js         --format=cjs  --minify=false",
    "build:iife:dev": "          npm run rollup -- --env=development              --ext=js                  --format=iife --minify=false",
    "build:iife:prod": "         npm run rollup -- --env=production               --ext=min.js              --format=iife --minify=true",
    "build:runtime:es:dev": "    npm run rollup -- --env=development --name=index --ext=runtime.esm.dev.js  --format=es   --minify=false                 --entry=src/runtime.ts",
    "build:runtime:es:prod": "   npm run rollup -- --env=production  --name=index --ext=runtime.esm.prod.js --format=es   --minify=false                 --entry=src/runtime.ts",
    "build:runtime:es": "        npm run rollup -- --env=unknown     --name=index --ext=runtime.esm.js      --format=es   --minify=false --replace=false --entry=src/runtime.ts",
    "build:runtime:cjs:dev": "   npm run rollup -- --env=development --name=index --ext=runtime.cjs.dev.js  --format=cjs  --minify=false                 --entry=src/runtime.ts",
    "build:runtime:cjs:prod": "  npm run rollup -- --env=production  --name=index --ext=runtime.cjs.prod.js --format=cjs  --minify=false                 --entry=src/runtime.ts",
    "build:runtime:iife:dev": "  npm run rollup -- --env=development              --ext=runtime.js          --format=iife --minify=false                 --entry=src/runtime.ts",
    "build:runtime:iife:prod": " npm run rollup -- --env=production               --ext=runtime.min.js      --format=iife --minify=true                  --entry=src/runtime.ts",
    "rollup": "lerna exec -- node ../../scripts/build.js",
    "types": "lerna exec -- node ../../scripts/typings.js",
    "_publish": "npm run build && lerna exec -- npm publish",
    "release": "npm run release-patch",
    "release-patch": "lenra version patch --force-publish && npm run _publish",
    "release-minor": "lerna version minor --force-publish && npm run _publish",
    "release-major": "lerna version major --force-publish && npm run _publish",
    "prelease": "lerna version prerelease --preid beta --force-publish && npm run _publish -- --tag=beta",
    "prelease-patch": "lerna version prepatch --preid beta --force-publish && npm run _publish -- --tag=beta",
    "prelease-minor": "lerna version preminor --preid beta --force-publish && npm run _publish -- --tag=beta",
    "prelease-major": "lerna version premajor --preid beta --force-publish && npm run _publish -- --tag=beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Javey/Intact.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Javey/Intact/issues"
  },
  "homepage": "https://github.com/Javey/Intact#readme",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.15.1",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/benchmark": "^2.1.0",
    "@types/chai": "^4.2.16",
    "@types/common-tags": "^1.8.0",
    "@types/karma-snapshot": "^0.6.0",
    "@types/mocha": "^8.2.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.5",
    "@types/webpack-env": "^1.16.0",
    "@vue/compiler-sfc": "^3.2.6",
    "benchmark": "^2.1.4",
    "chai": "^4.3.4",
    "chai-karma-snapshot": "^0.8.0",
    "common-tags": "^1.8.0",
    "css-loader": "^5.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^6.1.1",
    "karma-benchmark": "^1.0.4",
    "karma-benchmark-reporter": "^0.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-mocha-snapshot": "^0.2.1",
    "karma-sinon-chai": "^2.0.2",
    "karma-snapshot": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "lerna": "^4.0.0",
    "merge-dirs": "^0.2.1",
    "minimist": "^1.2.5",
    "mocha": "^8.3.2",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "tslib": "^2.2.0",
    "typescript": "^4.2.2",
    "vue": "^3.2.6",
    "vue-legacy": "npm:vue@^2.6.14",
    "vue-legacy-loader": "npm:vue-loader@^15.9.8",
    "vue-loader": "^16.5.0",
    "vue-router": "^4.0.11",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.24.2"
  }
}
