{
  "name": "intact-builder",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "benchmark": "karma start fixtures/karma.benchmark.js",
    "test": "karma start fixtures/karma.test.js",
    "snapshot": "UPDATE=1 npm run test",
    "clean": "rm -rf node_modules/.rpt2_cache && lerna exec -- rm -rf dist",
    "prebuild": "npm run clean",
    "build": "run-p build:**",
    "build:es:dev": "npm run rollup   -- --env=development --name=index --ext=dev.esm.js --format=es  --minify=false",
    "build:es:prod": "npm run rollup  -- --env=production  --name=index --ext=esm.js     --format=es  --minify=false",
    "build:cjs:dev": "npm run rollup  -- --env=development --name=index --ext=dev.cjs.js --format=cjs --minify=false",
    "build:cjs:prod": "npm run rollup -- --env=production  --name=index --ext=cjs.js     --format=cjs --minify=false",
    "build:umd:dev": "npm run rollup  -- --env=development              --ext=js         --format=umd --minify=false",
    "build:umd:prod": "npm run rollup -- --env=production               --ext=min.js     --format=umd --minify=true",
    "rollup": "lerna exec -- node ../../scripts/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Javey/Intact.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Javey/Intact/issues"
  },
  "homepage": "https://github.com/Javey/Intact#readme",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/benchmark": "^2.1.0",
    "@types/chai": "^4.2.16",
    "@types/common-tags": "^1.8.0",
    "@types/karma-snapshot": "^0.6.0",
    "@types/mocha": "^8.2.2",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.5",
    "benchmark": "^2.1.4",
    "chai": "^4.3.4",
    "chai-karma-snapshot": "^0.8.0",
    "common-tags": "^1.8.0",
    "css-loader": "^5.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^6.1.1",
    "karma-benchmark": "^1.0.4",
    "karma-benchmark-reporter": "^0.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-mocha-snapshot": "^0.2.1",
    "karma-sinon-chai": "^2.0.2",
    "karma-snapshot": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "lerna": "^4.0.0",
    "minimist": "^1.2.5",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.47.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "tslib": "^2.2.0",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2"
  }
}
